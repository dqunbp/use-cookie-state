    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "rollup": "^2.26.11",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
